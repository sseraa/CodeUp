#1018
a = input()
b = a.split(":")

# # 숫자 %d, 문자 %s
# # print("%s:%s" % (b[0],b[1]))
print("{}:{}".format(b[0],b[1]))

#1019
# a = input()
# b = a.split('.')
a = input().split('.')

yy = a[0].zfill(4)
mm = a[1].zfill(2)
dd = format(int(a[2]),'02')
print("{}.{}.{}".format(yy,mm,dd))

